<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE log4j:configuration SYSTEM "log4j.dtd">
<log4j:configuration xmlns:log4j='http://jakarta.apache.org/log4j/'>

	<appender name="CA" class="org.apache.log4j.ConsoleAppender">
		<layout class="org.apache.log4j.PatternLayout">
			<param name="ConversionPattern" value="%-6r %-5p %c{2} - %m%n" />
		</layout>
	</appender>



	<logger
		name="cz.agents.agentpolis.tools.geovisio.database.h2.H2GeometryTableInserter">
		<level value="off" />
		<appender-ref ref="CA" />
	</logger>

	<logger
		name="cz.agents.agentpolis.tools.geovisio.spy.agentpolis.SpyEventHandler">
		<level value="off" />
		<appender-ref ref="CA" />
	</logger>

	<logger
		name="cz.agents.agentpolis.tools.geovisio.spy.darptestbed.DarpTestbedDataReader">
		<level value="off" />
		<appender-ref ref="CA" />
	</logger>

	<logger
		name="cz.agents.agentpolis.tools.geovisio.spy.darptestbed.DarpTestbedDataReader">
		<level value="off" />
		<appender-ref ref="CA" />
	</logger>

	<logger
		name="cz.agents.agentpolis.simulator.importer.osm.tasks.GraphImportTask">
		<level value="off" />
		<appender-ref ref="CA" />
	</logger>


	<root>
		<level value="error" />
		<appender-ref ref="CA" />
		<!-- <appender-ref ref="FileA" /> -->
	</root>
</log4j:configuration>
